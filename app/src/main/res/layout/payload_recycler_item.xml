<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/payload_item_root">

    <TextView
            android:id="@+id/text_payload_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="200dp"
            android:ellipsize="end"
            android:textStyle="bold"
            android:gravity="center"
            tools:text="Nusantara Satu (PSN-6)"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"/>

    <TextView
            android:id="@+id/text_nationality"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="100dp"
            android:ellipsize="end"
            tools:text="Indonesia"
            app:layout_constraintTop_toTopOf="@+id/textView21"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toEndOf="@+id/barrier"
            android:layout_marginStart="8dp"/>
    <TextView
            android:id="@+id/textView21"
            android:text="@string/nationality"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="100dp"
            android:singleLine="true"
            android:ellipsize="end"
            app:layout_constraintTop_toBottomOf="@+id/text_payload_id" android:layout_marginTop="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"/>
    <TextView
            android:id="@+id/textView24"
            android:text="@string/manufacturer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="100dp"
            android:singleLine="true"
            android:ellipsize="end"
            app:layout_constraintStart_toStartOf="@+id/textView21"
            app:layout_constraintTop_toBottomOf="@+id/textView21"
            android:layout_marginTop="8dp"/>

    <TextView
            android:id="@+id/text_manufacturer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="100dp"
            android:singleLine="true"
            android:ellipsize="end"
            app:layout_constraintStart_toStartOf="@+id/text_nationality"
            app:layout_constraintTop_toBottomOf="@+id/textView21"
            android:layout_marginTop="8dp"
            tools:text="SSL"/>
    <TextView
            android:id="@+id/textView26"
            android:text="@string/payload_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="100dp"
            android:singleLine="true"
            android:ellipsize="end"
            app:layout_constraintStart_toStartOf="@+id/textView24"
            app:layout_constraintTop_toBottomOf="@+id/textView24"
            android:layout_marginTop="8dp"/>
    <TextView
            android:id="@+id/text_payload_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="100dp"
            android:singleLine="true"
            android:ellipsize="end"
            app:layout_constraintTop_toBottomOf="@+id/text_manufacturer"
            app:layout_constraintStart_toStartOf="@+id/text_manufacturer"
            android:layout_marginTop="8dp"
            tools:text="Satellite"/>
    <TextView
            android:id="@+id/textView28"
            android:text="@string/payload_mass_kg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="100dp"
            android:singleLine="true"
            android:ellipsize="end"
            app:layout_constraintTop_toBottomOf="@+id/textView26"
            app:layout_constraintStart_toStartOf="@+id/textView26"
            android:layout_marginTop="8dp"/>
    <TextView
            android:id="@+id/text_payload_mass_kg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="100dp"
            android:singleLine="true"
            android:ellipsize="end"
            app:layout_constraintTop_toBottomOf="@+id/text_payload_type"
            app:layout_constraintStart_toStartOf="@+id/text_payload_type"
            android:layout_marginTop="8dp"
            tools:text="5000 kg"/>
    <TextView
            android:id="@+id/textView30"
            android:text="@string/orbit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="100dp"
            android:singleLine="true"
            android:ellipsize="end"
            app:layout_constraintTop_toBottomOf="@+id/textView28"
            app:layout_constraintStart_toStartOf="@+id/textView28"
            android:layout_marginTop="8dp"/>
    <TextView
            android:id="@+id/text_orbit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="100dp"
            android:singleLine="true"
            android:ellipsize="end"
            app:layout_constraintTop_toBottomOf="@+id/text_payload_mass_kg"
            app:layout_constraintStart_toStartOf="@+id/text_payload_mass_kg"
            android:layout_marginTop="8dp" tools:text="GTO"/>
    <TextView
            android:id="@+id/textView32"
            android:text="@string/payload_reused"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="100dp"
            android:singleLine="true"
            android:ellipsize="end"
            app:layout_constraintTop_toBottomOf="@+id/textView30"
            app:layout_constraintStart_toStartOf="@+id/textView30"
            android:layout_marginTop="8dp"/>
    <TextView
            android:id="@+id/text_reused"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="@+id/text_orbit"
            app:layout_constraintTop_toBottomOf="@+id/text_orbit"
            android:layout_marginTop="8dp"
            tools:text="@string/yes"/>
    <TextView
            android:id="@+id/textView34"
            android:text="@string/customers_payloads"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxWidth="100dp"
            android:singleLine="true"
            android:ellipsize="end"
            app:layout_constraintTop_toBottomOf="@+id/textView32"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            android:layout_marginTop="16dp"/>
    <TextView
            android:id="@+id/text_customers"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:gravity="center"
            app:layout_constraintTop_toBottomOf="@+id/textView34"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            tools:text='Pasifik Satelit Nusantara"'
            android:layout_marginTop="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"/>

    <androidx.constraintlayout.widget.Barrier
            android:id="@+id/barrier"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:barrierDirection="right"
            app:constraint_referenced_ids="textView21, textView24, textView26, textView28, textView30, textView32"
            tools:layout_editor_absoluteX="104dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>